{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": [ "prettier", "jest"],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "react/jsx-filename-extension": [ 1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}