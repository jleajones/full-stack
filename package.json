{
  "name": "full-stack",
  "version": "1.0.0",
  "description": "Fullstack Javascript app built with React and Express",
  "main": "index.js",
  "scripts": {
    "serve:dev": "NODE_ENV=dev concurrently --kill-others-on-fail \"npm run start\" \"npm run client:dev\"",
    "serve": "NODE_ENV=dev concurrently --kill-others-on-fail \"npm run start\" \"npm run client\"",
    "start": "nodemon --exec babel-node server.js",
    "client:dev": "cd client && npm run build:dev && npm run start:dev",
    "client": "cd client && npm run build:prod && npm run start:prod",
    "test": "jest",
    "test:all": "jest && cd client && npm run test"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ]
  },
  "keywords": [
    "javascript",
    "node",
    "react",
    "express",
    "es6"
  ],
  "author": "J. Lea-Jones",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "convict": "^4.4.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "morgan": "^1.9.1",
    "stripe": "^6.20.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.3.0"
  }
}
