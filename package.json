{
  "name": "full-stack",
  "version": "1.0.0",
  "description": "Fullstack Javascript app built with React and Express",
  "main": "index.js",
  "scripts": {
    "serve:dev": "concurrently --kill-others-on-fail \"npm run start\" \"npm run client\"",
    "start": "nodemon --exec babel-node server.js",
    "client": "cd client && npm run build && npm run start:dev",
    "test": "test"
  },
  "keywords": [
    "javascript",
    "node",
    "react",
    "express",
    "es6"
  ],
  "author": "J. Lea-Jones",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9"
  }
}
